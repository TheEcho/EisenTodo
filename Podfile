  # Comment the next line if you're not using Swift and don't want to use dynamic frameworks
  use_frameworks!

  # Pods for EisenTodo
  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Firestore'
  pod 'GoogleSignIn'
  pod 'FacebookLogin'

  target 'EisenTodoTests' do
    inherit! :search_paths
    # Pods for testing
  end

  target 'EisenTodoUITests' do
    inherit! :search_paths
    # Pods for testing
  end


end
